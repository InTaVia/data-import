{
    "name": "@intavia/data-import",
    "version": "0.1.13",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "type": "module",
    "license": "MIT",
    "exports": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.modern.js"
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "engines": {
        "node": ">=18",
        "npm": ">=8"
    },
    "packageManager": "npm@8.19.2",
    "scripts": {
        "build": "rm -rf dist && microbundle --format cjs,modern ./src/index.ts",
        "changeset:add": "changeset add",
        "changeset:status": "changeset status --verbose",
        "ci:release": "npm run build && changeset publish",
        "generate:xlsx2json2intavia": "ts-node --project ./tsconfig.node.json ./scripts/xlsx2json2intavia",
        "test": "uvu -r tsm test"
    },
    "dependencies": {
        "@intavia/api-client": "^0.1.25",
        "buffer": "^6.0.3",
        "lodash.isequal": "^4.5.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.0",
        "@stefanprobst/key-by": "^1.0.1",
        "@types/lodash.isequal": "^4.5.6",
        "@types/prettier": "^2.7.2",
        "microbundle": "^0.15.1",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "tsm": "^2.3.0",
        "typescript": "^4.9.4",
        "uvu": "^0.5.6"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4
    }
}
